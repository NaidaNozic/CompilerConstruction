OWA {
  int main() {
    int answer;
    string name;
    print("Welcome to the One-Way Adventure! What is your name? ");
    name = readLine();
    print("Hello ") + print(name) + print(". Which way do you want to go? (1-3) ");
    answer = readInt();

    if(answer == 1 || answer == 2) {
      print("You fall into a hole.\n") + print(name) + print(" dies.\nGame Over!\n");
    } else {
      print("You defeat a goblin and rescue the princess. The princess turns into a wolf and eats you. Oh snap!\n");
      print(name) + print(" dies.\nGame Over!\n");
    }
    return 0;
  }
}